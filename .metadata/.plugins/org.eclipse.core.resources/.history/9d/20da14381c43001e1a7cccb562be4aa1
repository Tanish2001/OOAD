package com.aurionpro.model;

public class printUtil {
	public void printSalarySlip(Employee emp) {
		System.out.println("Salary of "+emp.name+" is "+EmployeeLogic.calculateTotalSalary(emp)+" and tax is "+EmployeeLogic.calculateTax(emp)+" imposed tax is "+this.tax+"% and annual salary is "+this.calculateTotalSalary()*12);
	}
}
